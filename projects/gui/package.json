{
  "name": "gui",
  "version": "0.1.0",
  "private": true,
  "main": "main/main.js",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"npm run generate && next dev\" \"electron .\"",
    "dev:prod": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"CURRENT_ENV=PROD npm run dev\" \"electron .\"",
    "build": "next build && electron-builder",
    "start": "next start",
    "generate": "npx tailwindcss -c tailwind.config.ts -o styles/output.css",
    "format": "prettier --write .",
    "lint": "next lint"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "electron-serve": "^1.3.0",
    "graphql": "^16.8.1",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^29.1.0",
    "electron-builder": "^24.12.0",
    "@next/eslint-plugin-next": "^14.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-unicorn": "^51.0.1",
    "jsonc-eslint-parser": "^2.4.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
